#include<stdio.h>
#include<stdlib.h>
#include<string.c>
#defne MAX 20
char stack[MAX];
int top=-1;
void push(char  value){
	stack[++top]=value;
}
int isMatch(char ch1, char ch2)
	if(ch1=='{'&&ch2=='}')
		return 1;
	else if(ch1=='['&&ch2=='}')
		return 1;
	else if(ch1=='('&&ch2==')')
		return 1;
	else
		return 0;
char pop(){
	char ch;
	if(top=-1){
		printf("not balanced\n");
		exit(0);
	}
	ch=stack[top];
	top--;
	return ch;
}
int isEmpty(){
	if(top==-1)
		return 1;
	else
		return 0;

